<%- include layouts/user_header.ejs %>


        <div class="col-sm-6 col-md-10 col-md-offset-1">
            <div class="panel panel-danger">
                <div class="panel-heading">
                    Tiempo Cumplido
                </div>
                <table class="table table-striped">
                    <thead>
                    <tr>
                        <th>N°</th>
                        <th>Nombre</th>
                        <th>Apellido</th>
                        <th>Tiempo Restante</th>
                    </tr>
                    </thead>
                    <tbody>

                    <%
                    if(ends.length){
                    for(var i = 0;i < ends.length; i++){
                    %>
                    <tr>
                        <td><%= ends[i].id%></td>
                        <td><%= ends[i].name%></td>
                        <td><%= ends[i].last_name%></td>
                        <td><div class="text-danger">TIEMPO CUMPLIDO</div></td>
                        <td><a href="/sudo_del/<%= ends[i].id%>" class="btn btn-danger">- Quitar</a></td>
                    </tr>

                    <%
                    }
                    } else{%>
                    <tr><td>-</td><td>-</td><td>-</td></tr>
                    <%
                    }
                    %>
                    </tbody>
                </table>
            </div>
        </div>
        <div class="col-sm-5 col-md-10 col-md-offset-1">
            <div class="panel panel-info">
                <div class="panel-heading">
                    En Curso
                </div>
                <table class="table table-striped">
                    <thead>
                    <tr>
                        <th>N°</th>
                        <th>Nombre</th>
                        <th>Apellido</th>
                        <th>Fin de visita</th>
                    </tr>
                    </thead>
                    <tbody>

                    <%
                    if(data.length){
                    for(var k = 0;k < data.length; k++){
                    %>
                    <tr>
                        <td><%= data[k].id%></td>
                        <td><%= data[k].name%></td>
                        <td><%= data[k].last_name%></td>
                        <td><%= new Date(data[k].date_f).toLocaleTimeString()%></td>
                        <td></td>
                        <td><a href="/sudo_del/<%= data[k].id%>" class="btn btn-danger">- Quitar</a></td>

                    </tr>

                    <%
                    }
                    } else{%>
                    <tr><td>No hay Jumpers asociados a esta venta</td><td>-</td><td>-</td><td>-</td><td>-</td></tr>
                    <% } %>
                    </tbody>
                </table>
            </div>
        </div>


        
<%- include layouts/footer.ejs %>